@{
    Layout = null;
}
<div ng-model="locationDetails">
    <h2>Location Details Test</h2>
        <div class="form-horizontal">
            <ul id="validationSummary"></ul>
            <div class="form-group">
                <label for="Image" class = "control-label col-md-2">
                Image
                </label>
                <div class="col-md-10">
                    {{data.Image}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Name" class = "control-label col-md-2">
                Name
                </label>
                <div class="col-md-10">
                    {{data.Name}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Trip" class = "control-label col-md-2">
                 Trip
                </label>
                <div class="col-md-10">
                    <a ng-href="#Trip?Location={{data.Id }}" >
                    {{data.Trip_Count}}
                    </a>
                </div>
            </div>
    
           
        </div>
    
    <div>
    </div>
</div>
